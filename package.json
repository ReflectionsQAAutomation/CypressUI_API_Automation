{
  "name": "toolshop",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "UI:E2E:test": "cypress run --spec cypress/e2e/**/*.cy.js --config-file demo.config.js --record --key 13b89e1a-0c85-4a6a-8392-ad72183afd56",
    "API:E2E:test": "cypress run  --spec 'cypress/e2e/API/*.cy.js' --browser chrome --record --key 13b89e1a-0c85-4a6a-8392-ad72183afd56",
    "clean:reports": "(if exist cypress\\results (rmdir /Q /S cypress\\results)) && mkdir cypress\\results && mkdir cypress\\results\\json  &&  mkdir cypress\\results\\junit",
    "pretest": "npm run clean:reports",
    "merge:json:report": "npx mochawesome-merge ./cypress/results/json/mochawesome*.json > ./cypress/results/json/output.json",
    "generate:html:report": "npx marge ./cypress/results/json/output.json --reportDir ./cypress/results/html-report --reportFilename index.html",
    "merge:reports": "jrm ./cypress/results/junit/report.xml \"cypress/results/junit/test-results*.xml\" ",
    "uploadReport": "node ./cypress/utils/uploadReport.js",
    "posttest": "merge:reports",
    "UI:E2E:test:merge:upload": "npm run pretest && npm-run-all UI:E2E:test --continue-on-error merge:reports merge:json:report generate:html:report",
    "API:test:merge:upload": "npm run pretest && npm-run-all API:E2E:test --continue-on-error merge:reports uploadReport",
    "cy:demo": "cypress open --config-file demo.config.js",
    "cy:stage": "cypress open --config-file stage.config.js",
    "jenkins:demo": "cypress run --spec cypress/e2e/**/*.cy.js --config-file demo.config.js",
    "jenkins:stage": "cypress run --spec cypress/e2e/*/* --config-file stage.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axe-core": "^4.7.0",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.0",
    "cypress-downloadfile": "^1.2.3",
    "cypress-multi-reporters": "^1.6.3",
    "dotenv": "^16.0.3",
    "jimp": "^0.22.10",
    "junit-report-merger": "^5.0.0",
    "lambdatest-cypress-cli": "^3.0.12",
    "marge": "^1.0.1",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome-merge": "^4.3.0",
    "neat-csv": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "rimraf": "^4.4.0"
  },
  "devDependencies": {
    "cypress": "^13.7.3",
    "cypress-failed-log": "^2.10.0"
  }
}